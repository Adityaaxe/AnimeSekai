                    {{#each Carousel}}
                    {{#if this.iscarousel}}
                    <div class="carousel-cell carousel1">
                        <a href="Watching/{{this._id}}">
                            <div class="slide-content">
                                <div>
                                    <h1 class="movie-title">{{this.title}}</h1>
                                    <p class="movie-des">
                                        {{this.description}}
                                    </p>
                                    <p class="movie-details">
                                        <i class="ph ph-closed-captioning"></i>{{this.sub}} &nbsp;&nbsp; <i class="fa fa-microphone" aria-hidden="true"></i> {{this.dub}} &nbsp;&nbsp; 
                                        <i class="fa fa-clock" aria-hidden="true"></i> {{this.duration}}
                                    </p>
                                    <button id="wtchbtn"><i class="fa fa-play-circle me-2" aria-hidden="true"></i>Play</button>
                                    <button id="wtchlstbtn"><i class="fa fa-plus-circle me-2" aria-hidden="true"></i>Watchlist</button>
                                </div>
                            </div>
                            <img class="img-fluid rounded" src="images/{{this.poster}}" alt="">
                        </a>
                    </div>
                    {{/if}}
                    {{/each}}
                </div>
